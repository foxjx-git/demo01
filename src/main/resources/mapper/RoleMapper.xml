<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.my.mydemo_01.mapper.RoleMapper">
    <select id="selectRoleByUserId" parameterType="java.lang.String" resultType="com.my.mydemo_01.entity.RoleEntity">
       select distinct *
       from  t_liu_role r
       join r_user_role ur on r.role_id = ur.role_id
       where ur.user_id =#{userId}
    </select>

</mapper>
